<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.11.4 -->
<interface>
  <requires lib="gtk" version="4.10"/>
  <requires lib="libadwaita" version="1.3"/>
  <template class="SwayNotificationCenterNotificationContent" parent="AdwBin">
    <child>
      <object class="GtkOverlay" id="overlay">
        <property name="css-classes">notification-background</property>
        <property name="hexpand">True</property>
        <child>
          <object class="GtkRevealer" id="close_revealer">
            <property name="child">
              <object class="GtkButton" id="close_button">
                <property name="child">
                  <object class="GtkImage">
                    <property name="icon-name">notifications-close-symbolic</property>
                    <property name="icon-size">normal</property>
                  </object>
                </property>
                <property name="css-classes">close-button
circular</property>
                <property name="margin-bottom">2</property>
                <property name="margin-end">4</property>
                <property name="margin-start">4</property>
                <property name="margin-top">2</property>
              </object>
            </property>
            <property name="halign">end</property>
            <property name="hexpand">True</property>
            <property name="transition-type">crossfade</property>
            <property name="valign">start</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="base_box">
            <property name="css-classes">notification</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkBox" id="default_action">
                <property name="css-classes">notification-content
notification-default-action</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkBox">
                    <child>
                      <object class="GtkImage" id="img">
                        <property name="css-classes">image</property>
                        <property name="margin-end">12</property>
                        <property name="pixel-size">64</property>
                        <property name="valign">center</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="margin-end">14</property>
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkBox">
                            <child>
                              <object class="GtkLabel" id="summary_label">
                                <property name="css-classes">summary</property>
                                <property name="ellipsize">end</property>
                                <property name="hexpand">True</property>
                                <property name="xalign">0.0</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="time_label">
                                <property name="css-classes">time</property>
                                <property name="valign">start</property>
                                <property name="xalign">0.0</property>
                                <property name="yalign">0.0</property>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel" id="body_label">
                            <property name="css-classes">body</property>
                            <property name="ellipsize">end</property>
                            <property name="hexpand">True</property>
                            <property name="natural-wrap-mode">word</property>
                            <property name="vexpand">True</property>
                            <property name="wrap">True</property>
                            <property name="wrap-mode">word-char</property>
                            <property name="xalign">0.0</property>
                            <property name="yalign">0.0</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkImage" id="body_image">
                    <property name="css-classes">body-image</property>
                    <property name="halign">center</property>
                  </object>
                </child>
                <child>
                  <object class="GtkProgressBar" id="progress_bar">
                    <property name="hexpand">True</property>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="inline_reply_box">
                    <property name="css-classes">inline-reply</property>
                    <property name="hexpand">True</property>
                    <child>
                      <object class="GtkEntry" id="inline_reply_entry">
                        <property name="css-classes">inline-reply-entry</property>
                        <property name="hexpand">True</property>
                        <property name="input-hints">emoji | spellcheck | uppercase-sentences</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="inline_reply_button">
                        <property name="css-classes">inline-reply-button</property>
                        <property name="valign">end</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
